groupId=org.codehaus.mojo
artifactId=versions-maven-plugin
display=${groupId}:${artifactId}
version=2.8.1

gitRepo=https://github.com/mojohaus/${artifactId}.git
gitTag=${artifactId}-${version}

tool=mvn
jdk=8
